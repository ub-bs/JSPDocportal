      <outgoing>validate_object.is_valid</outgoing>
    </serviceTask>
    <sequenceFlow id="cancel_object.end" sourceRef="cancel_object" targetRef="end"/>
    <sequenceFlow id="commit_object.end" sourceRef="commit_object" targetRef="end"/>
